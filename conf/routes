# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /jedisTest                  controllers.Application.jedisTest()
GET     /loginToSFDC                controllers.Application.loginToSFDC()
GET     /oauth                      controllers.Application.handleOAuth(code: String)
GET     /manager/home               controllers.Application.managerHome()
GET     /manager/test               controllers.Application.managerTest()

GET     /standings                  controllers.Standings.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
